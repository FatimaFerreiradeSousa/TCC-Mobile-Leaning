<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <navigation-rule>
        <from-view-id>/md-professor/cadastrarResposta.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>cadastrarPergunta</from-outcome>
            <to-view-id>/md-professor/cadastrarPergunta.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/md-professor/perguntasCadastradas.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>editarPergunta</from-outcome>
            <to-view-id>/md-professor/editarPergunta.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/md-professor/editarPergunta.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>editarResposta</from-outcome>
            <to-view-id>/md-professor/editarResposta.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/md-professor/editarResposta.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>editarPergunta</from-outcome>
            <to-view-id>/md-professor/editarPergunta.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/md-professor/testesCadastrados.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>editarTeste</from-outcome>
            <to-view-id>/md-professor/editarTeste.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
