<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <navigation-rule>
        <from-view-id>/moduloProfessor/disciplinas/cadExercicio.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>addQuestao</from-outcome>
            <to-view-id>/moduloProfessor/disciplinas/addQuestaoTeste.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/moduloProfessor/disciplinas/addQuestaoTeste.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>voltarPage</from-outcome>
            <to-view-id>/moduloProfessor/disciplinas/cadExercicio.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/moduloProfessor/disciplinas/testesCadastrados.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>mostrarTeste</from-outcome>
            <to-view-id>/moduloProfessor/disciplinas/respondeTeste.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/moduloProfessor/grupos/gruposGerenciados.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>pagInicialGrupo</from-outcome>
            <to-view-id>/moduloProfessor/grupos/pagInicialGrupo.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        
        <navigation-case>
            <from-outcome>indexAluno</from-outcome>
            <to-view-id>/indexAluno.xhtml</to-view-id>
        </navigation-case>
        
        <navigation-case>
            <from-outcome>indexProfessor</from-outcome>
            <to-view-id>/indexProfessor.xhtml</to-view-id>
        </navigation-case>
    
    </navigation-rule>
</faces-config>